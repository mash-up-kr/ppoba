{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "license": "UNLICENSED",
  "author": {
    "name": "Karl Saehun Chung",
    "email": "nycom13@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json --noEmit && npx webpack",
    "deploy:dev": "STAGE=dev make all",
    "dev": "ts-node src/entry.local.ts",
    "dev:debug": "node --require ts-node/register --inspect-brk src/entry.local.ts",
    "lint": "eslint . --ext .js,.ts",
    "log:dev": "aws logs tail /aws/lambda/backend-dev --since 60m",
    "prepare": "ts-patch install",
    "test": "NODE_ENV=test jest",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "eslintIgnore": [
    "*.js",
    "dist"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.(ts|js)"
    ],
    "transform": {
      "^.+.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.332.0",
    "@nestjs/common": "^9.4.1",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.1",
    "@nestjs/mongoose": "9.2.2",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/swagger": "^6.3.0",
    "@ppoba/types": "workspace:*",
    "@sentry/node": "^7.52.1",
    "@sentry/tracing": "^7.52.1",
    "@sindresorhus/is": "^5.3.0",
    "@vendia/serverless-express": "^4.10.3",
    "cache-manager": "^5.2.1",
    "case-anything": "^2.1.13",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "got": "^11.8.6",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "5.5.0",
    "mongoose": "7.2.3",
    "nanoid": "^3",
    "open": "^8",
    "pretty-error": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.1",
    "soft-delete-plugin-mongoose": "^1.0.15",
    "swagger-ui-dist": "^4.18.3",
    "swagger-ui-express": "^4.6.3",
    "typia": "^4.0.5",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.1",
    "@types/aws-lambda": "^8.10.115",
    "@types/cache-manager": "^4.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.1.7",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "typescript": "^4.9.5",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1"
  }
}
