{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist", "esm"]
    },
    "script#build": {
      "dependsOn": ["^build"],
      "outputs": ["../ppoba"]
    },
    "frontend#build": {
      "dependsOn": ["^build"],
      "outputs": [".next", ".open-next"]
    },
    "deploy:dev": {
      "dependsOn": ["build"]
    },
    "lint": {}
  }
}
